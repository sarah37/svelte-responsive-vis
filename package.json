		"topojson-client": "^3.1.0",
		"vega-embed": "^7.0.2"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"keywords": [
		"svelte"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
