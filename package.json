		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.36.0",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@types/node": "^24",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.13",
		"svelte": "^5.39.5",
		"svelte-vega": "^4.0.1",
		"svelteplot": "^0.4.9",
		"typescript": "^5.9.2",
		"vite": "^7.1.7"
	},
	"dependencies": {
		"d3-array": "^3.2.4",
		"d3-dsv": "^3.0.1",
		"d3-force": "^3.0.0",
		"d3-geo": "^3.1.1",
		"d3-hexjson": "^1.1.1",
